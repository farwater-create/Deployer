{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "rootDir": "src",
    "outDir": "build",
    "resolveJsonModule": true
  },
  "include": ["src/"],
  "exclude": ["node_modules/", "migrations/"]
}
