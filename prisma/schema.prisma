// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db.sqlite"
}

model OffensiveMinecraftSkin {
  hash String @unique
}

model FarwaterUser {
  discordId String
  minecraftUuid String? 
  minecraftName String?
  minecraftSkinSum String?
  age String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  minecraftApplications MinecraftApplication[]
  @@unique([discordId])
}

model MinecraftApplication {
  id Int @id @default(autoincrement())
  farwaterUser FarwaterUser @relation(fields: [discordId], references: [discordId])
  discordId String
  roleId String
  reason String
  minecraftServerId String
  status String
  createdAt DateTime @default(now())
  @@unique([discordId, minecraftServerId])
}